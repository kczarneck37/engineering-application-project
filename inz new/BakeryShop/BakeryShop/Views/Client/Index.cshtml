    <!-- Start team Area -->
    </div>
    @model IEnumerable<BakeryShop.Models.Client>
    @{
        ViewBag.Title = "Clients";
    }
    <section class="team-area section-gap" id="team">
        <div class="container">
            <div class="row d-flex justify-content-center">
                <div class="menu-content pb-70 col-lg-8">
                    <div class="title text-center">
                        <h1 class="mb-10">Our Lovely Clients</h1>
                        <p>We are here for you!.</p>
                    </div>
                </div>
            </div>
            <p>
                @Html.ActionLink("Create New", "Create")
            </p>
            <table class="table">
                <tr>
                    <th>
                        @Html.DisplayNameFor(x => x.Name)
                    </th>
                    <th>
                        @Html.DisplayNameFor(x => x.Email)
                    </th>
                    <th>
                        @Html.DisplayNameFor(x => x.Phone)
                    </th>
                    <th>
                        @Html.DisplayNameFor(x => x.Addres)
                    </th>
                </tr>
                @foreach (var emp in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(x => emp.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(x => emp.Email)
                        </td>
                        <td>
                            @Html.DisplayFor(x => emp.Phone)
                        </td>
                        <td>
                            @Html.DisplayFor(x => emp.Addres)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = emp.ClientId }) |
                            @Html.ActionLink("Detail", "Detail", new { id = emp.ClientId }) |
                            @Html.ActionLink("Delete", "Delete", new { id = emp.ClientId })
                        </td>
                    </tr>
                }
            </table>
    </section>
    <!-- End team Area -->
    <!-- Start item-category Area -->
    <section class="item-category-area section-gap">
        <div class="container">
            <div class="row d-flex justify-content-center">
